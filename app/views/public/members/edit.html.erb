<h1>プロフィール編集</h1>

<%= form_with model: [:public,@member], local: true do |f| %>
  <%= f.label :name, "アカウント名" %>
  <%= f.text_field :name %>

  <%= f.label :email, "メールアドレス" %>
  <%= f.email_field :email %>

  <%= f.label :favorite_game, "好きなゲームソフト" %>
  <%= f.text_field :favorite_game %>

  <%= f.submit "更新する" %>
<% end %>