<div>
  <%= form_with url: public_search_path, local: true, method: :get do |f| %>
    <%= f.text_field :key_word %>
    <%= f.select :model, options_for_select([["ユーザー","Member"],["レビュータイトル","Review"]]) %>
    <%= f.select :search, options_for_select([["完全一致","perfect_match"],["前方一致","forward_match"],
    ["後方一致","backward_match"],["部分一致","partial_match"]]) %>
    <%= f.submit "検索" %>
  <% end %>
</div>